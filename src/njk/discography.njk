{% extends '_layouts/base.njk' %}
{% block page_title %} Discography {% endblock %}
{% block content %}

<div class= "row">
    {% for discography in data.discography %}
    <div class="col-sm-3">
    <div class="card" style="width: 18rem;">
      <img src = "img/img/discs/{{ discography.image }}">
      <div class="card-body">
      <h5 class="card-title"> {{ discography.title }} </h5>
      <p class="card-text"> {{ discography.year }} </p>
    </div>
    </div>
    </div>
    {% endfor %}
    </div>
    </div>
{% endblock %}